

#**************************************************************
# Time Information
#**************************************************************

set_time_format -unit ns -decimal_places 3
set 300m00_period   3.333  ;# 300.00 MHz - Nom
set 330m00_period   3.030  ;# 330.00 MHz - Target 300MHz   1.1xNom
set 360m00_period   2.778  ;# 360.00 MHz - Target 300MHz   1.2xNom

set nom          1
set nom_plus_10p 0
set nom_plus_20p 0

if $nom          {
set clk_period       $300m00_period
}

if $nom_plus_10p {
set clk_period       $330m00_period
}

if $nom_plus_20p {
set clk_period       $360m00_period
}

#**************************************************************
# Create Clock
#**************************************************************

create_clock -name {clk} -period $clk_period [get_ports clk]




